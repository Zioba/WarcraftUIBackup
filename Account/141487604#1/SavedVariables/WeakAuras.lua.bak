					["custom"] = "aura_env.RuneColourChange = false -- Uses spec colours for runes, if disabled the colour used is the one in the disaply tab\naura_env.RuneTimer = true -- You can edit the text be editing the left text field \naura_env.CooldownColour = true -- Edit the colour using by entering the in the aura_env.RechargeColour field below colour below\naura_env.DynamicRunes = true -- Toggles dynamically sorted runes\n-- Font settings are taken dynamically from left text in the display tab\nlocal shadowx = 1 -- Changes the x position text shadow\nlocal shadowy = -1 -- changes the y position text shadow\n-- Spec Colours can be adjusted below, using a number between 0 and 1. e.g. 180 is 180/255 = 0.70\naura_env.RechargeColour = { -- Changes the colour of cooling down runes\n    [\"r\"] = 0.5,  -- red value\n    [\"g\"] = 0.9,  -- green value\n    [\"b\"] = 1,  -- blue value\n    [\"a\"] = 1,  -- alpha value\n    [\"br\"] = 0.2, -- red background value\n    [\"bg\"] = 0.3, -- green background value\n    [\"bb\"] = 0.5, -- blue background value\n    [\"ba\"] = 0.6  -- alpha background value\n}\naura_env.BloodColour = { -- Changes the colour of blood runes\n    [\"r\"] = 1,  -- red value\n    [\"g\"] = 0,  -- green value\n    [\"b\"] = 0,  -- blue value\n    [\"a\"] = 0.6,  -- alpha value\n    [\"br\"] = 0.3, -- red background value\n    [\"bg\"] = 0, -- green background value\n    [\"bb\"] = 0, -- blue background value\n    [\"ba\"] = 1  -- alpha background value\n}\naura_env.FrostColour = { -- Changes the colour of frost runes\n    [\"r\"] = 0,  -- red value\n    [\"g\"] = 1,  -- green value\n    [\"b\"] = 1,  -- blue value\n    [\"a\"] = 1,  -- alpha value\n    [\"br\"] = 0, -- red background value\n    [\"bg\"] = 0.3, -- green background value\n    [\"bb\"] = 0.3, -- blue background value\n    [\"ba\"] = 1  -- alpha background value\n}\naura_env.UnholyColour = { -- Changes the colour of unholy runes\n    [\"r\"] = 0,  -- red value\n    [\"g\"] = 0.81, -- green value