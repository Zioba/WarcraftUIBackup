					["colorFunc"] = "function(progress, r1, g1, b1, a1, r2, g2, b2, a2)\n    \n    local red, green, blue, alpha = 1, 1, 1, 1\n    local specID = GetSpecialization()\n    \n    if WeakAuras.IsOptionsOpen() then \n        \n        aura_env.DKRune2Duration = 1\n        aura_env.DKRune2Start = 1\n    end\n    \n    if aura_env and aura_env.state and aura_env.RuneTimer == true then\n        \n        local TimeRemaining = aura_env.DKRune2Duration - GetTime() + aura_env.DKRune2Start\n        local region = WeakAuras.regions[aura_env.id].region\n        local bar = region.bar\n        local textCenter = bar.centerText\n        local runer, runeg, runeb, runea = WeakAuras.regions[aura_env.id].region.text:GetTextColor()\n        local ExpFontName, ExpFontHeight, ExpFontFlags = WeakAuras.regions[aura_env.id].region.text:GetFont()\n        \n        if TimeRemaining > 5 then\n            \n            textCenter:SetText(string.format(\"%.0f\", TimeRemaining))\n            \n        elseif TimeRemaining < 5 and TimeRemaining > 0 then\n            \n            textCenter:SetText(string.format(\"%.1f\", TimeRemaining))\n            \n        else \n            textCenter:SetText(\"\")\n            \n            textCenter:SetFont(\n                ExpFontName,\n                ExpFontHeight,\n                ExpFontFlags\n            )\n            \n            textCenter:SetTextColor(\n                runer, \n                runeg, \n                runeb, \n                runea\n            )\n            \n            textCenter:SetPoint(\n                \"CENTER\",\n                bar,\n                \"CENTER\",\n                0,\n                0\n            )\n            textCenter:Show()\n        end\n    end\n    if aura_env and aura_env.state and aura_env.CooldownColour == true and aura_env.DKRune2Start ~= 0 then\n        \n        red, green, blue, alpha =  aura_env.RechargeColour.r, aura_env.RechargeColour.g, aura_env.RechargeColour.b, aura_env.RechargeColour.a\n        WeakAuras.regions[aura_env.id].region.bar.bg:SetVertexColor(aura_env.RechargeColour.br, aura_env.RechargeColour.bg, aura_env.RechargeColour.bb, aura_env.RechargeColour.ba);\n        return red, green, blue, alpha\n    elseif aura_env.RuneColourChange == true then\n        \n        if specID == 1 then\n            \n            red, green, blue, alpha = aura_env.BloodColour.r, aura_env.BloodColour.g, aura_env.BloodColour.b, aura_env.BloodColour.a\n            WeakAuras.regions[aura_env.id].region.bar.bg:SetVertexColor(aura_env.BloodColour.br, aura_env.BloodColour.bg, aura_env.BloodColour.bb, aura_env.BloodColour.ba);\n            return red, green, blue, alpha\n        elseif specID == 2 then\n            \n            red, green, blue, alpha = aura_env.FrostColour.r, aura_env.FrostColour.g, aura_env.FrostColour.b, aura_env.FrostColour.a\n            WeakAuras.regions[aura_env.id].region.bar.bg:SetVertexColor(aura_env.FrostColour.br, aura_env.FrostColour.bg, aura_env.FrostColour.bb, aura_env.FrostColour.ba);\n            return red, green, blue, alpha\n        elseif specID == 3 then\n            \n            red, green, blue, alpha = aura_env.UnholyColour.r, aura_env.UnholyColour.g, aura_env.UnholyColour.b, aura_env.UnholyColour.a\n            WeakAuras.regions[aura_env.id].region.bar.bg:SetVertexColor(aura_env.UnholyColour.br, aura_env.UnholyColour.bg, aura_env.UnholyColour.bb, aura_env.UnholyColour.ba);\n            return red, green, blue, alpha\n        end\n        \n    else \n        red, green, blue = r1, g1, b1\n        return red, green, blue, 1\n    end\nend",
